{
	"name" : "lpetrora/equilibrium-rest",
	"description" : "The Equilibrim framework for ReST apis",
	"license": "MIT",
	"authors" : [{
			"name" : "Leonardo Petrora"
		}
	],
	"require" : {
		"monolog/monolog" : "^3",
		"smarty/smarty" : "^4",
		"propel/propel" : "dev-master"
	},
	"minimum-stability": "dev",
	"prefer-stable" : true,
	"autoload" : {
		"classmap" : ["src/Equilibrium.php"],
		"psr-4" : {
			"equilibrium\\" : "src/",
			"app\\" : "../../../"
		}
	},
	"bin": [
		"src/bin/equilibrium",
		"src/bin/equilibrium.bat"
	],
	"keywords" : [
		"framework"
	]
}
